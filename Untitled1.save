int my_getdelim ( char *buffer, int *size, char *specifier, FILE *f )
{
    char temp[50]    = {};
    char buffer[200] = {};
    
    
    while ( temp[0] != "\0" ) {
        fscanf ( f, "%s", &temp );
        strcat ( buffer, temp );
    }
    
    return 0;
}
        